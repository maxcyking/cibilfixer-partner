{
    "version": 2,
    "buildCommand": "flutter build web --release --web-renderer html",
    "outputDirectory": "build/web",
    "installCommand": "if [ ! -d \"/opt/flutter\" ]; then git clone https://github.com/flutter/flutter.git -b stable /opt/flutter; fi && export PATH=\"$PATH:/opt/flutter/bin\" && flutter doctor && flutter pub get",
    "framework": null,
    "routes": [
        {
            "src": "/assets/(.*)",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
            "headers": {
                "cache-control": "public, max-age=31536000, immutable"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}